#include <stdio.h>
#include "main.h"

/**
* main - prints the last digit of the random
* number stored in the variable n
* Return: Always 0 (Success)
* the string and is greater than 5
* the string and is 0
* the string and is less than 6 and not 0
*/
int _islower(int c)
{
if ('a' <= c <= 'z')
return (1);
else
return (0);
}
